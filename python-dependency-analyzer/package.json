{
  "name": "python-dependency-analyzer-pro",
  "version": "1.0.0",
  "description": "Enterprise-grade Python dependency analysis with ISO 42001, GDPR & EU AI Act compliance",
  "type": "module",
  "author": "Aegisia",
  "license": "MIT",
  "keywords": [
    "dependency-analysis",
    "security",
    "compliance",
    "iso-42001",
    "gdpr",
    "ai-act",
    "python",
    "risk-assessment"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ghjulia01/aegisia/tree/main/python-dependency-analyzer"
  },
  "scripts": {
    "dev": "vite --host --port 3000",
    "build": "tsc && vite build",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview --port 3000",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,css,md}\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "clean": "rm -rf dist node_modules/.vite",
    "validate": "npm run typecheck && npm run lint"
  },
  "dependencies": {
    "d3": "^7.8.5",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.263.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.15.0",
    "react-router-dom": "^6.14.1"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^2.1.9",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.15.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.6.3",
    "vite": "^6.2.3",
    "vitest": "2.1.9"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
